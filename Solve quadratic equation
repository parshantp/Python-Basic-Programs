
# import complex math module


import cmath
a = float(input("Enter a :-"))
b = float(input("Enter b :-"))
c = float(input("Enter c :-"))

# calculate the discriminant
d = (b**2) - (4*a*c)

# find two solutions
s1 = (-b-cmath.sqrt(d))/(2*a)
s2 = (-b+cmath.sqrt(d))/(2*a)

print(f"The solutions are {s1} and {s2}")
